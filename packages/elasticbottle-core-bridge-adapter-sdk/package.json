{
  "name": "@elasticbottle/core-bridge-adapter-sdk",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --env.ESM_ONLY true",
    "build-packages": "pnpm build",
    "dev": "tsup src/index.ts --env.ESM_ONLY true --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@elasticbottle/bridge-adapter-sdk-tsconfig": "^0.0.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/web3.js": "^1.78.0",
    "eslint": "^8.29.0",
    "eslint-config-bridge-adapter-sdk": "^0.0.0",
    "ethers": "^5.7.2",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "undici": "^5.22.1",
    "viem": "^1.4.1"
  },
  "dependencies": {
    "@certusone/wormhole-sdk": "^0.9.22",
    "@mayanfinance/swap-sdk": "^4.2.3",
    "@solana/spl-token": "^0.3.8",
    "csv42": "^4.0.0",
    "valibot": "^0.3.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.78.0",
    "ethers": "^5.7.2",
    "viem": "^1.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
