    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-window": "^1.8.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.29.0",
    "eslint-config-bridge-adapter-sdk": "^0.0.0",
    "ethers": "^5.7.2",
